LOG2A=/data/data/com.mkadp.blapptwo/files/zbutterlimits.log
LOG2B=/data/data/com.mkadp.blapptwo/files/zzbutterlimits.log
FRACTION=75
RAM=$(free -m | awk '/Mem:/{print $2}')
CPUS=`grep -c processor /proc/cpuinfo`
SIZE=$(((RAM * FRACTION / 100) * 1048576))
pme='pm enable'
pmd='pm disable'
#=======================================#
#=======================================#

am start -a android.intent.action.MAIN -e toasttext "Removing old Logs" -n mkadp.toast/.MainActivity

rm $NEWLOG2
